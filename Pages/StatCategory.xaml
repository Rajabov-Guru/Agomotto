<Page x:Class="Agamotto.Pages.StatCategory"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Agamotto.Pages" xmlns:oxy="http://oxyplot.org/wpf"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StatCategory">

    <Grid>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="97" Foreground="#FFDEDE00" Height="32" FontSize="16" FontWeight="Bold" Click="BackClick">
            <Button.Background>
                <ImageBrush/>
            </Button.Background>
        </Button>
        <oxy:PlotView x:Name="gone" Margin="10,47,10,10" Background="{x:Null}" Foreground="Black" BorderBrush="White" >

        </oxy:PlotView>
    </Grid>
</Page>
